{
  "expo": {
    "name": "deo-iuvante-native",
    "description": "This project is really great.",
    "slug": "deo-iuvante-native",
    "privacy": "public",
    "sdkVersion": "27.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": true,
        "NSCameraUsageDescription": true,
        "NSPhotoLibraryAddUsageDescription": true
      }
    },
    "android": {
      "package": "com.pewpew.pistch",
      "permissions": ["CAMERA", "MANAGE_DOCUMENTS", "READ_CONTACTS"]
    }
  }
}
